{
   "unicast" : "@BENCH_IP_MASTER@",
   "logging" : 
   {
      "level" : "debug",
      "console" : "true",
      "dlt" : "false"
   },

   "applications" : 
   [
      {
         "name" : "bench_publish_subscribe_publisher",
         "id" : "0x2001"
      }
   ],

    "services" : 
   [
      {
         "service" : "0x1234",
         "instance" : "0x5678",
         "unreliable" : "30509",
         "reliable" :
         {
            "port" : "30510",
            "enable-magic-cookies" : "false"
         },
         "events" :
         [
            {
               "event" : "0x8771",
               "is_field" : "false"
            },
            {
               "event" : "0x8772",
               "is_field" : "false",
               "is_reliable" : "true"
            },
            {
               "event" : "0x8779",
               "is_field" : "false"
            }
         ],
         "eventgroups" :
         [
            {
               "eventgroup" : "0x4465",
               "events" : [ "0x8771", "0x8779" ],
               "multicast" :
               {
                  "address" : "224.225.226.233",
                  "port" : "32344"
               },
               "threshold" : "1"
            },
            {
               "eventgroup" : "0x4466",
               "events" : [ "0x8772" ]
            }
         ]
      }
   ],

   "routing" : "bench_publish_subscribe_publisher",
   "service-discovery" : 
   {
      "enable" : "true",
      "multicast" : "224.0.0.1",
      "port" : "30490",
      "protocol" : "udp"
   },

   "service-security" :
   {
      "certificates-path" : "@CRYPTO_CERTIFICATES_FOLDER_RELATIVE@",
